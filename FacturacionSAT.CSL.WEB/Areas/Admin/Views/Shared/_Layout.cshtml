<!DOCTYPE html>
<html lang="en">
<head>
    <!-- META SECTION -->
    <title>Viajes Aury - Administrador Web</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="Sistema Administrador" />
    <meta name="Creativa Softline" />
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    <link rel="icon" href="@Url.Content("~/Content/favicon.ico")" type="image/x-icon" />
    <!-- END META SECTION -->
    <!-- CSS INCLUDE -->
    <link rel="stylesheet" type="text/css" id="theme" href="@Url.Content("~/Content/css/theme-default.css")" />
    @RenderSection("css", required: false)
    <!-- EOF CSS INCLUDE -->
</head>
<body>
    <!-- START PAGE CONTAINER -->
    <div class="page-container">
        <!-- START PAGE SIDEBAR -->
        <div class="page-sidebar scroll">
            <!-- START X-NAVIGATION -->
            <ul class="x-navigation">
                <li class="xn-logo">
                    <a href="@Url.Action("Index", "HomeAdmin")">FACTURACIÓN</a>
                    <a href="#" class="x-navigation-control"></a>
                </li>
                <li class="xn-profile">
                    <a href="#" class="profile-mini ">
                        <img src="http://www.viajesaury.com/images/logo.png" alt="Viajes Aury" />
                    </a>
                    <div class="profile">
                        <div class="profile-image">
                            <img src="http://www.viajesaury.com/images/logo.png" alt="Viajes Aury" />
                        </div>
                        <div class="profile-data">
                            <div class="profile-data-name">VIAJES AURY FACTURACIÓN</div>
                            @*@{
                                    HttpContext context = HttpContext.Current;
                                    if (context != null && context.Session != null)
                                    {
                                        string NombreUsuario = (string)HttpContext.Current.Session["NombreUsuario"];
                                        <div class="profile-data-name">@NombreUsuario</div>
                                    }
                                }*@
                        </div>
                        @*<div class="profile-controls">
                                <a href="pages-profile.html" class="profile-control-left"><span class="fa fa-info"></span></a>
                                <a href="pages-messages.html" class="profile-control-right"><span class="fa fa-envelope"></span></a>
                            </div>*@
                    </div>
                </li>
                <li class="xn-title">Menu</li>
                <li>
                    <a href="#"><span class="fa fa-desktop"></span> <span class="xn-text">PAC</span></a>
                </li>
                <li>
                    <a href="#"><span class="fa fa-desktop"></span> <span class="xn-text">CONCEPTOS</span></a>
                </li>
                <li>
                    <a href="#"><span class="fa fa-desktop"></span> <span class="xn-text">OTROS</span></a>
                </li>
            </ul>
            <!-- END X-NAVIGATION -->
        </div>
        <!-- END PAGE SIDEBAR -->
        <!-- PAGE CONTENT -->
        <div class="page-content">

            <!-- START X-NAVIGATION VERTICAL -->
            <ul class="x-navigation x-navigation-horizontal x-navigation-panel">
                <!-- TOGGLE NAVIGATION -->
                <li class="xn-icon-button">
                    <a href="#" class="x-navigation-minimize"><span class="fa fa-dedent"></span></a>
                </li>
                <!-- END TOGGLE NAVIGATION -->
                <!-- SEARCH -->
                @*<li class="xn-search">
                        <form role="form">
                            <input type="text" name="search" placeholder="Search..." />
                        </form>
                    </li>*@
                <!-- END SEARCH -->
                <!-- SIGN OUT -->
                <li class="xn-icon-button pull-right">
                    <a class="mb-control" data-box="#mb-signout"><span class="fa fa-sign-out"></span></a>
                </li>
                <!-- END SIGN OUT -->
                <!-- MESSAGES -->
                <!-- END MESSAGES -->
                <!-- TASKS -->
                <!-- END TASKS -->
            </ul>
            <ul class="breadcrumb">
                <center><li class="active">SISTEMA ADMINISTRADOR FACTURA</li></center>
            </ul>
            <!-- END X-NAVIGATION VERTICAL -->
            <!-- START BREADCRUMB -->
            <div class="page-content-wrap">
                @RenderSection("featured", required: false)
            </div>
            <!-- END PAGE CONTENT WRAPPER -->
        </div>
        <!-- END PAGE CONTENT -->
    </div>
    <!-- END PAGE CONTAINER -->
    <!-- MESSAGE BOX-->
    <div class="message-box animated fadeIn" data-sound="alert" id="mb-signout">
        <div class="mb-container">
            <div class="mb-middle">
                <div class="mb-title"><span class="fa fa-sign-out"></span> Cerrar <strong>Sesión</strong> ?</div>
                <div class="mb-content">
                    <p>Seguro que quiere cerrar sesión?</p>
                    <p>Presione No si desea continuar trabajando. Presione Sí para desconectarse del usuario actual.</p>
                </div>
                <div class="mb-footer">
                    <div class="pull-right">
                        <a href="@Url.Action("LogOff", "Account", new { Area = "Admin"})" class="btn btn-success btn-lg">Si</a>
                        <button class="btn btn-default btn-lg mb-control-close">No</button>
                    </div>
                </div>
            </div>
        </div>
        @RenderBody()
    </div>
    <!-- END MESSAGE BOX-->
    <!-- START SCRIPTS -->
    <!-- START PLUGINS -->
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/jquery/jquery.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/jquery/jquery-ui.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/bootstrap/bootstrap.min.js")"></script>
    <!-- END PLUGINS -->
    <script src="@Url.Content("https://maps.googleapis.com/maps/api/js?key=AIzaSyB5BKT7iWoiumqmmh2ex_ixcg9QMLte5VY&v=3.exp&libraries=places")"></script>
    <script type='text/javascript' src="@Url.Content("~/Content/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js")"></script>
    <script type='text/javascript' src="@Url.Content("~/Content/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js")"></script>
    <script type='text/javascript' src="@Url.Content("~/Content/js/plugins/jvectormap/jquery-jvectormap-europe-mill-en.js")"></script>
    <script type='text/javascript' src="@Url.Content("~/Content/js/plugins/jvectormap/jquery-jvectormap-us-aea-en.js")"></script>
    <!-- START THIS PAGE PLUGINS-->
    <script type='text/javascript' src="@Url.Content("~/Content/js/plugins/icheck/icheck.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/scrolltotop/scrolltopcontrol.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/rickshaw/d3.v3.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/rickshaw/rickshaw.min.js")"></script>
    <script type='text/javascript' src="@Url.Content("~/Content/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js")"></script>
    <script type='text/javascript' src="@Url.Content("~/Content/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js")"></script>
    <script type='text/javascript' src="@Url.Content("~/Content/js/plugins/bootstrap/bootstrap-datepicker.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/owl/owl.carousel.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/moment.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins/daterangepicker/daterangepicker.js")"></script>
    @RenderSection("script", required: false)
    @*<script type="text/javascript" src="@Url.Content("~/Content/js/settings.js")"></script>*@
    <script type="text/javascript" src="@Url.Content("~/Content/js/plugins.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/actions.js")"></script>
    <script src="@Url.Content("~/Content/js/demo_maps.js")"></script>
    <script src="@Url.Content("~/Content/js/plugins/daterangepicker/bootstrap-datepicker.es.js")" charset="UTF-8"></script>
    <!-- END TEMPLATE -->
    <!-- END SCRIPTS -->
</body>
</html>

