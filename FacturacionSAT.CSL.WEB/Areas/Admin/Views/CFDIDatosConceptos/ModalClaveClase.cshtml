@model FacturacionSAT.CSL.WEB.Models.CFDIConceptoClaseModels

<div class="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">CFDI Clase</h4>
    </div>
    <div class="modal-body">

        <table id="dtClaveClase" class="display nowrap" style="width:100%">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Clave Clase SAT</th>
                    <th>Clase</th>
                </tr>
            </thead>
        </table>

        <div class="modal-footer">

            <button id="btnGuardarClaveClase" type="button" class="btn btn-success green">Guardar</button>
            <button type="button" class="btn btn-secondary red" data-dismiss="modal">Cancelar</button>

        </div>
    </div>

</div>




