@model FacturacionSAT.CSL.WEB.Models.CFDIConceptoClaveUnidadModels

<div class="modal-Seleccione">
    <div class="modal-headerSeleccione">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title"><strong>Clave unidad</strong></h4>
    </div>
    <div class="modal-body">
        @using (Html.BeginForm("AC_Evento", "Compra", FormMethod.Post, new { @class = "form-horizontal", id = "frmEvento", autocomplete = "off" }))
        {
            <table id="dtClaveUnidad" class="display nowrap" style="width:100%">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Clave unidad SAT</th>
                        <th>Nombre</th>
                    </tr>
                </thead>
            </table>
        }
        <div class="modal-footer">
            
                <button id="btnGuardarClaveUnidad" type="button" class="btn btn-success green">Guardar</button>
                <button type="button" class="btn btn-secondary red" data-dismiss="modal">Cancelar</button>
            
        </div>
    </div>

</div>


