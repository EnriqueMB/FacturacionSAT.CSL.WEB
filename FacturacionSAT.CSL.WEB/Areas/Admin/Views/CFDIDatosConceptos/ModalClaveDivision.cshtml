@model FacturacionSAT.CSL.WEB.Models.CFDIConceptoDivicionModels

<div class="modal-Seleccione">
    <div class="modal-headerSeleccione">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title"><strong>CFDI división</strong></h4>
    </div>
    <div class="modal-body">
       
            <table id="dtClaveDivision" class="display nowrap" style="width:100%">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Clave División SAT</th>
                        <th>Nombre</th>
                    </tr>
                </thead>
            </table>
      
        <div class="modal-footer">

            <button id="btnGuardarClaveDivision" type="button" class="btn btn-success green">Guardar</button>
            <button type="button" class="btn btn-secondary red" data-dismiss="modal">Cancelar</button>

        </div>
    </div>

</div>



